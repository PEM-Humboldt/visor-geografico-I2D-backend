[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */tests/*
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py
    */urls.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\(Protocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
